{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": true,
		"isolatedModules": true,
		"composite": true,
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"strict": true,
		"outDir": "./dist"
	},
	"extends": "./.svelte-kit/tsconfig.json"
}
